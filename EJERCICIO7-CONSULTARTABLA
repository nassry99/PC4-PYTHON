
import re
import 

def consultar_archivo_por_mes(archivo, mes):
    try:
        with open(archivo, 'r') as file:
            for linea in file:
                # Buscar la fecha en formato YYYY-DD-MM en la l√≠nea
                fecha = re.search(r'\d{4}-\d{2}-\d{2}', linea)
                if fecha:
                    # Extraer el mes de la fecha encontrada
                    mes_linea = fecha.group(0)[8:10]
                    # Verificar si el mes coincide con el mes proporcionado por el usuario
                    if mes_linea == mes:
                        print(linea.strip())
    except FileNotFoundError:
        print(f"El archivo '{archivo}' no existe.")

def main():
    archivo = "sunat_dolar_202403.txt"
    mes = input("Ingrese el mes (en formato 'MM') que desea consultar: ")
    consultar_archivo_por_mes(archivo, mes)

if __name__ == "__main__":
    main()

